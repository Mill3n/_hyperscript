<html lang="en">
<head>
    <title>///_hyperscript</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/css/site.css"/>
    <link rel="stylesheet" href="/css/prism-htmx.css"/>
    <script src="https://unpkg.com/prismjs@1.20.0/components/prism-core.min.js"></script>
    <script src="https://unpkg.com/prismjs@1.20.0/plugins/autoloader/prism-autoloader.min.js"></script>
    <script src="/js/prism-hyperscript.js"></script>
    <script src="/js/lib/core.js"></script>
    <script src="/js/lib/web.js"></script>
    <script src="/js/lib/worker.js"></script>
    <script src="/js/lib/hdb.js"></script>
</head>
<body>
<div class="top-nav">
    <div class="c">
        <div class="menu">
            <div class="logo-wrapper">
                <a href="/"><img height="36px" src="/img/light_logo.png"></a>
                <svg onclick="document.getElementById('nav').classList.toggle('show')" class="hamburger" viewBox="0 0 100 80" width="25" height="25" style="margin-bottom:-5px">
                    <rect width="100" height="20" style="fill:rgb(52, 101, 164)" rx="10"></rect>
                    <rect y="30" width="100" height="20" style="fill:rgb(52, 101, 164)" rx="10"></rect>
                    <rect y="60" width="100" height="20" style="fill:rgb(52, 101, 164)" rx="10"></rect>
                </svg>
            </div>
            {% if page.url.indexOf("/examples/") == 0 %}
            <div id="nav" class="navigation nav"> <!-- don't boost on demo pages, sinon hijacks everything :/ -->
                {% else %}
                <div id="nav" class="navigation" hx-boost="true">
                    {% endif %}
                    <div class="navigation-items" preload="mouseover">
                        <div>
                            <a href="/docs/">docs</a>
                        </div>
                        <div>
                            <a href="/reference/">reference</a>
                        </div>
                        <div>
                            <a href="/cookbook/">cookbook</a>
                        </div>
                        <div>
                            <a href="/talk/">talk</a>
                        </div>
                        <div>
                            <a href="https://github.com/sponsors/bigskysoftware?o=esb">sponsor</a>
                        </div>
                    </div>
                    <div class="github-stars">
                        <a href="https://github.com/bigskysoftware/_hyperscript">github</a>
                        <iframe style="margin:auto;" src="https://ghbtns.com/github-btn.html?user=bigskysoftware&repo=_hyperscript&type=star&count=true" frameborder="0" scrolling="0" width="150" height="20" title="Star htmx on GitHub"></iframe>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="c content">
    {{ content | safe }}
</div>
</body>
</html>
